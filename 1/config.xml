<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.micronetonline.appmap" version="3.1.8" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AppMapp - Williston Business Directory</name>
    <description>
    AppMapp - Williston Business Directory
  </description>
    <author email="you@example.com" href="http://example.com.com/">&#xD;&#xD;
    Micronet&#xD;&#xD;
  </author>
    <content src="index.html"/>
